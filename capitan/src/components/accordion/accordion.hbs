<div class="accordion {{class}}">
    {{#each items}}
        <div class="accordion__trigger">
            <span>{{title}}</span>
        </div>
        <div class="accordion__content-wrapper">
            <div class="row">
                <div class="col-sm-12">
                    <p>{{content}}</p>
                </div>
            </div>
        </div>
    {{/each}}
</div>

